{
  "name": "@fed-ea/core-typescript",
  "version": "1.0.0-alpha.2",
  "description": "TypeScript representation of FED-EA core data model.",
  "main": "lib/definitions-core.js",
  "types": "types/src/definitions-core.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "dev:tsc:spec": "tsc --watch --inlineSourceMap --project tsconfig.spec.json",
    "dev:build:spec": "bunx bun build ./out/spec/test.spec.js --watch --target node --sourcemap --outdir ./bundle --define global=window --external bun:test",
    "dev:test:watch": "bunx bun test --watch ./bundle/test.spec.js",
    "prod:tsc:dist": "tsc --project tsconfig.dist.json --clean --version",
    "prod:build:dist": "bunx bun build ./out/src/definitions-core.js --target node --outdir ./dist",

    "npm:audit": "npm audit",
    "npm:outdated": "npm outdated"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fed-ea/core-typescript.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fed-ea/core-typescript/issues"
  },
  "homepage": "https://github.com/fed-ea/core-typescript#readme",
  "devDependencies": {
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "typed-assert": "^1.0.9"
  }
}
