{
  "name": "@fed-ea/typescript-core",
  "version": "0.0.1",
  "description": "JSON schema and Typescript types which are functional equivalents of Archimate Exchange Format.",
  "main": "lib/definitions.js",
  "types": "types/definitions.d.ts",
  "files": [
    "lib",
    "types"
  ],
  "scripts": {
    "tsc:watch": "tsc --watch",
